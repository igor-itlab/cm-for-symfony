{
    "name": "itlab-studio/acceptcoin-api",
    "description": "Library for api call with different resources",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AcceptcoinApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcceptcoinApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ITLab-Studio Development",
            "email": "igor-tolstoy@itlab-studio.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "itlab-studio/acceptcoin-api",
                "version": "master",
                "source": {
                    "url": "https://github.com/igor-itlab/cm-for-symfony.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/http-kernel": "*",
        "symfony/dependency-injection": "*",
        "symfony/config": "*",
        "symfony/http-client": "*",
        "doctrine/annotations": "*",
        "symfony/serializer": "*",
        "symfony/property-access": "*",
        "doctrine/collections": "*",
        "symfony/event-dispatcher": "*",
        "symfony/validator": "^5.3",
        "web-token/jwt-framework": "^2.1",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/property-info": "^5.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "ext-dom": "*"
    },
    "extra": {
        "symfony": {
            "require": "^3.4 || ^4.0 || ^5.0"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3",
        "phpunit/phpunit": "^9.6",
        "symfony/test-pack": "^1.0"
    }
}
